		"Ractor.Persistence": "0.*",
	},
	"buildOptions": {
		"emitEntryPoint": false,
		"define": [],
		"compile": [ "../../src/Ractor/**/*.cs" ]
	},

	"tools": {
		"dotnet-compile-fsc": {
			"version": "1.0.0-preview2-*",
